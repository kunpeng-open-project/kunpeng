<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kunpeng.framework.modules.project.mapper.ProjectMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.kunpeng.framework.modules.project.po.ProjectPO">
        <id column="project_id" property="projectId" />
        <result column="create_date" property="createDate" />
        <result column="create_user_id" property="createUserId" />
        <result column="create_user_name" property="createUserName" />
        <result column="update_date" property="updateDate" />
        <result column="update_user_id" property="updateUserId" />
        <result column="update_user_name" property="updateUserName" />
        <result column="delete_flag" property="deleteFlag" />
        <result column="project_name" property="projectName" />
        <result column="project_code" property="projectCode" />
        <result column="project_url" property="projectUrl" />
        <result column="status" property="status" />
        <result column="manage" property="manage" />
        <result column="app_id" property="appId" />
        <result column="app_secret" property="appSecret" />
        <result column="token_failure" property="tokenFailure" />
        <result column="token_gain_max_num" property="tokenGainMaxNum" />
        <result column="voucher" property="voucher" />
        <result column="remark" property="remark" />
    </resultMap>

    <!-- 表字段结果列 -->
    <sql id="Base_Column_List">
        project_id, project_name, project_code, project_url, status, manage, app_id, app_secret, token_failure, token_gain_max_num, voucher, remark
    </sql>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_Common_List">
        create_date, create_user_id, create_user_name, update_date, update_user_id, update_user_name, delete_flag
    </sql>

</mapper>
